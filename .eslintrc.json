{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:jsdoc/recommended",
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint",
        "jsdoc"
    ],
    "rules": {
        "require-jsdoc": 0,
        "valid-jsdoc": "off",
        "jsdoc/require-jsdoc": [
            "error",
            {
                "publicOnly": false,
                "require": {
                    "FunctionExpression": true,
                    "ArrowFunctionExpression": true,
                    "FunctionDeclaration": true,
                    "MethodDefinition": true
                }
            }
        ]
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
